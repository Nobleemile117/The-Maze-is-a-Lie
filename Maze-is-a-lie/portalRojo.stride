<?xml version="1.0" encoding="UTF-8"?>
<class name="portalRojo" extends="Actor" extends-java="Actor" enable="true" strideversion="1">
    <javadoc xml:space="preserve"/>
    <imports>
        <import target="java.util.*" enable="true"/>
        <import target="greenfoot.*" enable="true"/>
    </imports>
    <implements/>
    <fields/>
    <constructors/>
    <methods>
        <method access="public" type="void" type-java="void" name="act" enable="true">
            <javadoc xml:space="preserve">Act - do whatever the portalRojo wants to do. This method is called whenever the 'Act' or 'Run' button gets pressed in the environment.</javadoc>
            <params/>
            <throws/>
            <body>
                <if condition="isTouching(Chell.class)" condition-java="isTouching(Chell.class)" enable="true">
                    <ifStatements>
                        <call expression="Greenfoot.setWorld(new ScoreScreen())" expression-java="Greenfoot.setWorld( new ScoreScreen())" enable="true"/>
                    </ifStatements>
                </if>
            </body>
        </method>
    </methods>
</class>
